<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/backgroundTemporary">
        <shape android:shape="rectangle">
            <solid android:color="@color/default_color_game_transparent" />
            <corners android:radius="7dp"/>
        </shape>
    </item>

    <item
        android:id="@+id/frontTemporary"
        android:start="2dp"
        android:end="2dp"
        android:top="3dp"
        android:bottom="3dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/dark" />
            <corners android:radius="7dp"/>
        </shape>
    </item>
</layer-list>
