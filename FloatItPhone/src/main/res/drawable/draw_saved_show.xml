<ripple
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/default_color">

    <item
        android:id="@+id/backtemp"
        android:bottom="5dp"
        android:left="4dp"
        android:right="5dp"
        android:top="5dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/default_color"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_saved_show"/>

    <item
        android:id="@android:id/mask"
        android:bottom="4dp"
        android:left="3dp"
        android:right="4dp"
        android:top="4dp">
        <shape
            android:shape="oval">
            <solid
                android:color="@color/default_color"/>
        </shape>
    </item>
</ripple>